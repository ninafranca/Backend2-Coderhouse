<h1>Productos</h1>
<div class="div-products">
    {{#if products}}
        {{#each products}}
            <div>
                <h2>{{this.title}}</h2>
                <h2>${{this.price}}</h2>
                <img src="{{this.thumbnail}}" alt="Product image" style="width: 20vw">
            </div>
        {{/each}}
    {{else}}
        <h2>No hay productos</h2>
    {{/if}}
</div>