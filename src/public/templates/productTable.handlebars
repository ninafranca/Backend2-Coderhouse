<h1 id="title-productos">Productos</h1>
<div class="div-products">
    <div class="div-div-products">
        {{#if products}}
        {{#each products}}
            <div class="products-title">
                <h2>{{this.title}}</h2>
                <h2>${{this.price}}</h2>
                <img src="{{this.thumbnail}}" alt="Product image" style="width: 10vw">
            </div>
        {{/each}}
        {{else}}
            <h2>No hay productos</h2>
        {{/if}}
    </div>
</div>